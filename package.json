    "devDependencies": {
        "@babel/preset-env": "^7.18.6",
        "@rollup/plugin-babel": "^5.3.1",
        "@rollup/plugin-node-resolve": "^13.3.0",
        "rollup-plugin-copy": "^2.0.1",
        "rollup-plugin-terser": "^7.0.2",
        "rufflib-expect": "^2.0.1",
        "rufflib-validate": "^1.3.0"
    }
